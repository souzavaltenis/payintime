<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/primary">

    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/primary" />
            <corners android:radius="1000dp"/>
        </shape>
    </item>

    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="1000dp"/>
        </shape>
    </item>

    <item>
        <inset
            android:inset="4dp"
            android:drawable="@drawable/ic_baseline_menu_24">
        </inset>
    </item>

</ripple>